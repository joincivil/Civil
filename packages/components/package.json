{
  "name": "@joincivil/components",
  "version": "1.7.5",
  "description": "React components for Civil",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "license": "Apache-2.0",
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testPathIgnorePatterns": ["/node_modules/", "build"],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$":
        "<rootDir>src/__mocks__/fileMock.js"
    },
    "testRegex": "(src/__test__/.*|\\.(test|spec))\\.(ts|tsx)$",
    "setupTestFrameworkScriptFile": "<rootDir>src/__test__/setupTests.js",
    "moduleFileExtensions": ["ts", "tsx", "js", "json"]
  },
  "scripts": {
    "copy:images": "mkdir -p build/images && cp -a src/images/. build/images",
    "build": "yarn clean && tsc && yarn copy:images",
    "build:watch": "yarn clean && yarn copy:images && tsc -w",
    "lint": "tslint --exclude \"**/storyFixtures/**\" --project ./",
    "clean": "rimraf build/",
    "prepublish": "run-s build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "storybook:build": "build-storybook -c .storybook -o out",
    "test": "NODE_ENV=test jest"
  },
  "devDependencies": {
    "@joincivil/typescript-types": "^1.3.1",
    "@storybook/addon-storyshots": "^3.3.14",
    "@storybook/react": "^3.3.14",
    "@types/enzyme": "^3.1.9",
    "@types/jest": "^23.3.5",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.39",
    "@types/react-dom": "^16.0.4",
    "@types/react-test-renderer": "^16.0.1",
    "@types/storybook__react": "^3.0.7",
    "awesome-typescript-loader": "^3.5.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.4",
    "core-js": "^2.5.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.5.0",
    "npm-run-all": ">=4.1.5",
    "react-test-renderer": "^16.2.0",
    "rimraf": "^2.6.2",
    "storybook-react-router": "^1.0.1",
    "strip-ansi": "^5.0.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^4",
    "typescript": "<2.9.0",
    "webpack": "^4",
    "webpack-hot-middleware": "^2.21.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "prop-types": "^15.6.1",
    "react": "^16.4",
    "react-dom": "^16.4"
  },
  "dependencies": {
    "@joincivil/core": "^4.7.5",
    "@joincivil/ethapi": "^0.3.4",
    "@joincivil/utils": "^1.8.3",
    "@types/classnames": "^2.2.3",
    "apollo-storybook-react": "^0.1.8",
    "classnames": "^2.2.5",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.3",
    "lodash": "^4.17.10",
    "react-add-to-calendar": "^0.1.5",
    "react-apollo": "^2.3.3",
    "react-async-script": "^1.0.0",
    "react-router-dom": "^4.3.1",
    "react-rte": "^0.16.1",
    "rxjs": "^5.5.6",
    "styled-components": "^3.2.3"
  }
}
