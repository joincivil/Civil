{
  "name": "@joincivil/sdk",
  "version": "2.0.0",
  "private": false,
  "main": "build/package/index.js",
  "types": "build/package/index.d.ts",
  "dependencies": {
    "@babel/core": "7.2.2",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@joincivil/components": "^1.9.10",
    "@joincivil/core": "^4.8.11",
    "@joincivil/elements": "^0.0.1",
