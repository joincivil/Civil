    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "babel-preset-react-app": "^8.0.0",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "css-loader": "1.0.0",
    "detect-browser": "^4.7.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "eslint": "5.12.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "eth-crypto": "^1.3.3",
    "ethers": "^4.0.27",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.1",
    "graphql-tag": "^2.9.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "iframe-resizer": "^4.2.6",
    "ipfs-http-client": "^29.1.1",
    "jest": "23.6.0",
    "jest-pnp-resolver": "1.0.2",
    "jest-resolve": "23.6.0",
    "jest-watch-typeahead": "^0.2.1",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.15",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.2.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.5.0",
    "postcss-safe-parser": "4.0.1",
    "react": "^16.11.0",
    "react-apollo": "^2.3.3",
    "react-app-polyfill": "^0.2.2",
    "react-async-script": "^1.1.1",
    "react-dev-utils": "^8.0.0",
    "react-dom": "^16.11.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^4.0.0",
    "resolve": "1.10.0",
    "rxjs-compat": "^6.5.2",
    "sass-loader": "7.1.0",
    "source-map-explorer": "^1.8.0",
    "style-loader": "0.23.1",
    "terser-webpack-plugin": "1.2.2",
    "url-loader": "1.1.2",
    "uuid": "^3.3.2",
    "webpack": "4.28.3",
    "webpack-dev-server": "3.1.14",
    "webpack-manifest-plugin": "2.0.4",
    "workbox-webpack-plugin": "3.6.3"
  },
  "scripts": {
    "build": "yarn clean && yarn sdk:build && yarn package:build",
    "build:watch": "yarn package:build:watch",
    "sdk:start": "node scripts/start.js",
    "sdk:start:boost-embed": "OPEN_PATH=boost-embed.html node scripts/start.js",
    "sdk:build": "node scripts/build.js",
    "package:build": "yarn package:clean && yarn copy:images && tsc",
    "package:build:watch": "yarn copy:images && tsc -w",
    "package:clean": "rimraf build/package",
    "clean": "rimraf build",
    "test": "node scripts/test.js",
    "lint": "tslint --project ./",
    "copy:images": "mkdir -p build/package/images && cp -a src/images/. build/package/images",
    "prepublishOnly": "yarn build",
    "prettier": "prettier --config .prettierrc.yaml --write --list-different './**/*.{ts,tsx,json,md}'",
    "prettier:lint": "prettier --config .prettierrc.yaml --list-different './**/*.{ts,tsx,json,md}'",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "release": "semantic-release",
    "commit": "commit",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/plugin-transform-member-expression-literals": "^7.2.0",
    "@commitlint/prompt-cli": "^7.5.0",
    "@joincivil/tslint-rules": "^2.7.1",
    "@semantic-release/npm": "^5.1.7",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-links": "^5.0.10",
    "@storybook/addons": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@storybook/theming": "^5.0.10",
    "@types/iframe-resizer": "^3.5.8",
    "@types/jest": "24.0.11",
    "@types/js-cookie": "^2.2.1",
    "@types/lodash": "^4.14.136",
    "@types/node": "11.11.4",
    "@types/react": "16.8.8",
    "@types/react-dom": "16.8.3",
    "@types/react-router": "^5.1.1",
    "@types/react-stripe-elements": "^1.2.0",
    "@types/storybook__react": "^4.0.1",
    "@types/uuid": "^3.4.4",
    "apollo-storybook-react": "^0.2.1",
    "create-cert": "^1.0.6",
    "husky": "^2.1.0",
    "jest-fetch-mock": "^2.1.2",
    "mock-socket": "^8.0.5",
    "node-webcrypto-ossl": "^1.0.42",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "rimraf": "^2.6.2",
    "semantic-release": "^15.13.3",
    "tslint": "^5.17.0",
    "typescript": "^3.6.2"
  },
  "peerDependencies": {
    "styled-components": ">= 3"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupTests.ts",
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/../../node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename.js",
      "jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
